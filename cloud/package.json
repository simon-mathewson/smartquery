{
  "name": "dabase-cloud",
  "private": true,
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "NODE_ENV=development pnpm dotenv-flow -- tsx watch main",
    "lint": "eslint --cache .",
    "postinstall": "prisma generate",
    "prisma": "NODE_ENV=development pnpm dotenv-flow -- prisma",
    "setup-production": "chmod +x ./scripts/setup-production.sh && ./scripts/setup-production.sh",
    "stripe-webhook": "stripe listen --forward-to http://localhost:4444/stripe",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.848.0",
    "@google/genai": "0.12.0",
    "@noble/ciphers": "^1.3.0",
    "@prisma/client": "6.9.0",
    "@trpc/server": "11.1.4",
    "cookie": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "stripe": "18.4.0",
    "superjson": "1.12.4",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.13.5",
    "dotenv-flow-cli": "^1.1.1",
    "esbuild": "^0.17.10",
    "eslint": "^9.26.0",
    "globals": "^16.2.0",
    "prisma": "6.9.0",
    "rollup": "^4.42.0",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228",
  "prisma": {
    "seed": "tsx prisma/seeds/seed.ts"
  }
}
